{
  "name": "taro-mobile-web-client",
  "version": "1.0.0",
  "private": true,
  "description": "Smart speaking Web UI",
  "scripts": {
    "prestart": "npm run font-icon",
    "start": "npm run dev:h5",
    "test": "jest --coverage",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "font-icon": "icon-font-generator src/assets/svgs/**/*.svg -o src/assets/font-icons/ --csstp src/assets/icon-css-tmpl.hbs"
  },
  "lint-staged": {
    "**/*.scss": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-push": "./scripts/pre-push-hook.sh"
    }
  },
  "author": "smart speaking team",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "1.3.0-beta.5",
    "@tarojs/components": "1.3.0-beta.5",
    "@tarojs/redux": "1.3.0-beta.5",
    "@tarojs/redux-h5": "1.3.0-beta.5",
    "@tarojs/router": "1.3.0-beta.5",
    "@tarojs/taro": "1.3.0-beta.5",
    "@tarojs/taro-alipay": "1.3.0-beta.5",
    "@tarojs/taro-h5": "1.3.0-beta.5",
    "@tarojs/taro-swan": "1.3.0-beta.5",
    "@tarojs/taro-tt": "1.3.0-beta.5",
    "@tarojs/taro-weapp": "1.3.0-beta.5",
    "axios": "^0.19.0",
    "husky": "^2.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nerv-devtools": "^1.4.0-beta.3",
    "nervjs": "^1.4.0-beta.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "seamless-immutable": "^7.1.4",
    "taro-ui": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@tarojs/plugin-babel": "1.3.0-beta.5",
    "@tarojs/plugin-csso": "1.3.0-beta.5",
    "@tarojs/plugin-sass": "1.3.0-beta.5",
    "@tarojs/plugin-uglifyjs": "1.3.0-beta.5",
    "@tarojs/webpack-runner": "1.3.0-beta.5",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/react": "^16.4.6",
    "@types/redux-actions": "^2.6.1",
    "@types/seamless-immutable": "^7.1.10",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "cypress": "^3.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-taro": "1.3.0-beta.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.0-beta.5",
    "icon-font-generator": "^2.1.10",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.3.0",
    "jest-svg-transformer": "^1.0.0",
    "nerv-server": "^1.4.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "redux-actions": "^2.6.5",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.0-beta.5",
    "stylelint-taro-rn": "1.3.0-beta.5",
    "ts-jest": "^24.0.2",
    "typescript": "^3.0.1",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
