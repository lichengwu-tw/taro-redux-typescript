@font-face {
    font-family: "{{ fontName }}";
    src: {{{ src }}};
}

i, .icon {
    line-height: 1;
}

i:before, .icon:before {
    font-family: {{fontName}} !important;
    font-style: normal;
    font-weight: normal !important;
}

{{# each codepoints }}
.{{ ../classPrefix }}{{ @key }}:before {
    content: "\\{{ this }}";
}
{{/ each }}
